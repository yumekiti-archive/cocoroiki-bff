{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::app-user.app-user"
    },
    "content": {
      "type": "richtext"
    },
    "kids": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::app-user.app-user"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "like": {
      "type": "integer"
    },
    "reactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::app-user.app-user"
    }
  }
}
